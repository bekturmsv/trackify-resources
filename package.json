    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.18",
    "@types/pidusage": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^8.57.1",
